{
  "name": "@holochain-open-dev/calendar-events",
  "version": "0.0.2",
  "description": "Holochain reusable module to include calendar events in your happ",
  "author": "guillem.cordoba@gmail.com",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "build": "tsc && npm run analyze -- --exclude dist",
    "prepublish": "tsc && npm run analyze -- --exclude dist",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "publish-to-branch": "npm run lint && npm run build && gh-pages -d ./ -b ui-build"
  },
  "dependencies": {
    "@holochain-open-dev/cell-client": "^0.3.2",
    "@holochain-open-dev/core-types": "^0.2.0",
    "@holochain-open-dev/profiles": "^0.1.0",
    "@holochain/client": "^0.3.2",
    "@lit-labs/context": "^0.1.1",
    "@open-wc/scoped-elements": "^2.1.0",
    "@scoped-elements/material-web": "^0.0.19",
    "@scoped-elements/fullcalendar": "^0.0.2",
    "@scoped-elements/shoelace": "^0.0.8",
    "lit": "^2.0.2",
    "lit-svelte-stores": "^0.2.0",
    "svelte": "^3.43.2"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^2.0.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@web/dev-server": "0.1.17",
    "@web/dev-server-rollup": "^0.3.4",
    "@web/test-runner": "^0.9.7",
    "@web/test-runner-puppeteer": "^0.7.2",
    "buffer": "^5.7.1",
    "concurrently": "^5.1.0",
    "cqfill": "^0.6.0",
    "deepmerge": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "gh-pages": "^3.1.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-postcss-lit": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "tslib": "^1.11.0",
    "typescript": "^4.2.4",
    "web-component-analyzer": "^1.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
