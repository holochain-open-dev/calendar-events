{
  "name": "@holochain-open-dev/calendar-events",
  "version": "0.0.2",
  "description": "Holochain reusable module to include calendar events in your happ",
  "author": "guillem.cordoba@gmail.com",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm:start-ui\" \"npm:start-holochain\"",
    "start-ui": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k --names tsc,dev-server \"npm run build-watch\" \"sleep 5 && web-dev-server --config demo/web-dev-server.config.mjs\"",
    "start-holochain": "RUST_LOG=warn hc s g -r=8888 ../zome/workdir/happ/sample.happ",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "build-bundle": "rimraf bundle && rollup -c rollup-bundle.config.js",
    "build-watch": "rimraf dist && rollup -c rollup.config.js -w",
    "test": "npm run build && web-test-runner --coverage",
    "test-debug": "npm run build && DEBUG=true web-test-runner --coverage",
    "test-watch": "web-test-runner --watch",
    "e2e": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k -s first \"npm:test\" \"npm:start-holochain\"",
    "e2e-debug": "DEBUG=true npm run e2e",
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "storybook": "npm run analyze && concurrently -k --names tsc,storybook \"npm run build-watch\" \"start-storybook --node-resolve --watch --open\"",
    "build-storybook": "npm run analyze && build-storybook",
    "publish-storybook": "npm run lint && npm run analyze && npm run build-storybook && gh-pages -d storybook-static",
    "publish-npm": "npm run lint && npm run build && npm publish --access public && rimraf dist/",
    "publish-to-branch": "npm run lint && npm run build && gh-pages -d ./ -b ui-build"
  },
  "dependencies": {
    "@adobe/lit-mobx": "^2.0.0-rc.4",
    "@compository/lib": "git+https://github.com/compository/lib.git#build",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fullcalendar/core": "^5.7.0",
    "@fullcalendar/daygrid": "^5.7.0",
    "@fullcalendar/interaction": "^5.7.0",
    "@fullcalendar/timegrid": "^5.7.0",
    "@holochain-open-dev/context": "git+https://github.com/holochain-open-dev/context.git#ui-build",
    "@holochain-open-dev/core-types": "git+https://github.com/holochain-open-dev/core-types.git#build",
    "@holochain/client": "^0.3.2",
    "@lit-labs/scoped-registry-mixin": "^1.0.0-rc.1",
    "@webcomponents/scoped-custom-element-registry": "0.0.1",
    "bootstrap": "^4.5.2",
    "lit": "^2.0.0-rc.2",
    "mobx": "^6.3.2",
    "scoped-material-components": "^0.1.6"
  },
  "devDependencies": {
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^2.0.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@web/dev-server": "0.1.17",
    "@web/dev-server-rollup": "^0.3.4",
    "@web/test-runner": "^0.9.7",
    "@web/test-runner-puppeteer": "^0.7.2",
    "buffer": "^5.7.1",
    "concurrently": "^5.1.0",
    "cqfill": "^0.6.0",
    "deepmerge": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "gh-pages": "^3.1.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-postcss-lit": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "tslib": "^1.11.0",
    "typescript": "^4.2.4",
    "web-component-analyzer": "^1.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
